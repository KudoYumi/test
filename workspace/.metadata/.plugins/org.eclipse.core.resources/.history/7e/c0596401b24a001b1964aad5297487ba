package exe10;

class Sum extends Calculation{

	public Sum(double[] dt) {
		super(dt);
	}

	@Override
	public double calculate() {
		int sum=0;

	/*	for(int i=0; i<super.getDt().length; i++) {
			sum += super.getDt()[i];
		} */

		for(double d:super.getDt()) {
			sum += d;
		}

		return
	}

	public double average() {
		return
	}
}
