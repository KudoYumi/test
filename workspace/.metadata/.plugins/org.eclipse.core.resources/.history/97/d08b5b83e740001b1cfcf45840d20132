package zohoRPG1_2;

public class Main {

	public static void main(String[] args) {
		Fighter f = new Fighter("山田","戦士");
		Monster m = new Monster(40);

		System.out.println("モンスターがあらわれた");

		while(m.hp>0) {
			System.out.println
			(f.job+f.name+"はどうする？[1:たたかう][0:にげる]>");
		}
	}

}
