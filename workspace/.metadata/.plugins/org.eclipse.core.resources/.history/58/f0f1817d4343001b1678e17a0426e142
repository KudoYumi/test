package zoho20_17;

public abstract class Human {
	String name;
	String job;
	int hp;
	int mp;
	double dmg;

	//コンストラクタ
	Human(String name,String job,int hp,int mp,double dmg){
		this.name = name;
		this.job = job;
		this.hp = hp;
		this.mp = mp;
		this.dmg = dmg;
	}

	//攻撃メソッド
	public abstract void attack(Monster m);
}
