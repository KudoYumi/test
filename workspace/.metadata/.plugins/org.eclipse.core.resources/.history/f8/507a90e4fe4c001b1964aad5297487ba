package youtubeラムダの話8;

public class Item {
	private String id;
	private String name;
	private String description;
	private int price;


	public Item(String id, String name, String description, int price) {
		super();
		this.id = id;
		this.name = name;
		this.description = description;
		this.price = price;
	}
	/* 引数が多いという問題がある。
	 * これをもっとわかりやすくしたい！
	 */

}
