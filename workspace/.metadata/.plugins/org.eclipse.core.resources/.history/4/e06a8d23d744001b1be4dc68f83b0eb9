package zoho19_02;

class Member {
	private int id;
	private String name;
	private int monthlyPrice;

	Member(int id,String name){
		this.id = id;
		this.name = name;
		this.monthlyPrice = 5000;
	}

	public String kind() {
		return "一般";
	}

	public int payment() {
		return (int)(monthlyPrice*1.08);
	}
}
