package わかりやすいJava1演習問題;

class Seiseki {

	private String number;
	private String name;
	private int[] score;
	private boolean attend;

	public Seiseki(String number,String name,int[] score,boolean attend) {
		this.number = number;
		this.name = name;
		this.score = score;
		this.attend = attend;
	}
}
