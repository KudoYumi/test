package zoho20_16_3;
import java.util.*;

class Fighter { //ざっくり言って全員戦士
	String job;
	String name;
	int Dmg;

	Fighter(String name,int Dmg){
		this.job = "戦士";
		this.Dmg = Dmg;
	}

	void attack(Monster m) {
		System.out.println(job+name+"の攻撃");
		Dmg = Random().nextInt(10)+1;
	}
}
