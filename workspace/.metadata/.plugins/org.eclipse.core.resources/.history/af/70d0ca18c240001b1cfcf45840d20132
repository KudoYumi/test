package zoho12_3;

public class Main {
	static String[] name = {"山田","鈴木","佐々木","田中","小林"};
	static String[] gender = {"男","女","男","男","女"};
	static int[] age = {20,37,45,31,52};
	static String[] adr = {"東京","神奈川","東京","埼玉","埼玉"};


	public static void main(String[] args) {
		System.out.print("検索したい住所");
		String search_adr = new java.util.Scanner(System.in).nextLine();

		System.out.print("\n"+"検索したい性別");
		String search_gender = new java.util.Scanner(System.in).nextLine();
	}

	public static void show() {
		for(int i=0; i<5; i++) {
			System.out.println
				((i+1)+name[i]+gender[i]+age[i]+adr[i]);
		}
	}

}
