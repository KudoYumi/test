package zoho14_02;

public class Main {

	public static void main(String[] args) {
		int n = new java.util.Random().nextInt();
		Game g = new Game(n);

		System.out.println("好きな数字を入力＞");
		int myNum = new java.util.Scanner(System.in).nextInt();

		while(n==myNum) {
			g.playGame(myNum);
		}
	}

}
