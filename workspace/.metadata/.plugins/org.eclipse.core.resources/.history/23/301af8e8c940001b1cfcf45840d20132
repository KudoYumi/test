package zoho10_1;

class TestScore {
	private int number;
	private String name;
	int score;
	String judge;

	TestScore(int num,String name,int score,String judge){
		this.number = num;
		this.name = name;
		this.score = score;
		this.judge = judge;
	}

	int getScore() {return this.score;}

	void show() {
		System.out.println
		(number+"\t"+name+"\t"+score+"\t"+judge);
	}
}
