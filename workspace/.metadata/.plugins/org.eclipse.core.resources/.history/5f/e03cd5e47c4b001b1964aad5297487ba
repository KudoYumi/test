package コレクション;

import java.util.LinkedHashSet;
import java.util.Set;

public class Sample3 {
	public static void main(String[] args) {
		Set<String> colors = new LinkedHashSet<>();

		colors.add("赤");
		colors.add("黄");
		colors.add("青");
		colors.add("赤");

		System.out.println(colors.size());

		for(String s:colors) {
			System.out.println(s);
		}
	}
}
