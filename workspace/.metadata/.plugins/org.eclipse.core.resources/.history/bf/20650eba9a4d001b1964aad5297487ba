package equalsの復讐;

public class Item {
	private String name;
	private String color;
	private int price;

	public Item name(String name) {
		this.name = name;
		return this;
	}

	public Item color(String color) {
		this.color = color;
		return this;
	}

	public Item price(int price) {
		this.price = price;
		return this;
	}

	public boolean equals(Object obj) {

	}
}
